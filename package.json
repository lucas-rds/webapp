{
  "name": "webapp",
  "version": "1.0.0",
  "description": "Project for learning purposes",
  "main": "server.js",
  "scripts": {
    "start": "babel-node server/server.js",
    "start:livereload": "nodemon server/server.js --exec babel-node",
    "build": "babel server -d dist",
    "deploy": "node dist/server.js",
    "serve": "npm run build && npm run deploy",
    "test": "npx mocha --require babel-core/register --require babel-polyfill --exit ./test/**.test.js",
    "test:database": "npm run migrations && npx mocha --require babel-core/register --require babel-polyfill --exit ./test/database/**.test.db.js",
    "createdb": "npx sequelize db:create",
    "migrations": "npx sequelize db:migrate"
  },
  "license": "ISC",
  "homepage": "https://github.com/lucas-rds/webapp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lucas-rds/webapp.git"
  },
  "bugs": {
    "url": "https://github.com/lucas-rds/webapp/issues"
  },
  "keywords": [
    "graphql",
    "heroku",
    "angular",
    "express"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "author": "Lucas Rodrigues",
  "dependencies": {
    "chai": "^4.1.2",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "mocha": "^5.1.0",
    "pg": "^7.4.1",
    "sequelize": "^4.37.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.19.1",
    "husky": "^0.14.3",
    "sequelize-cli": "^4.0.0"
  }
}